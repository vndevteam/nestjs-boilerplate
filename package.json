{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "NestJS Boilerplate, Typescript, Postgres, TypeORM",
  "author": "vndevteam",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "env-cmd typeorm-ts-node-commonjs -d src/database/data-source.ts",
    "migration:up": "pnpm typeorm migration:run",
    "migration:down": "pnpm typeorm migration:revert",
    "migration:show": "pnpm typeorm migration:show",
    "migration:create": "typeorm migration:create",
    "migration:generate": "pnpm typeorm migration:generate --pretty",
    "prepare": "husky",
    "upgrade-deps": "ncu -u && pnpm install",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@nestjs/common": "10.3.9",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.9",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "10.3.9",
    "@nestjs/typeorm": "10.0.2",
    "argon2": "0.40.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "nestjs-i18n": "^10.4.5",
    "pg": "8.12.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/js": "9.5.0",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.9",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.4",
    "@types/supertest": "6.0.2",
    "@vuepress/bundler-vite": "2.0.0-rc.13",
    "@vuepress/plugin-search": "2.0.0-rc.34",
    "@vuepress/theme-default": "2.0.0-rc.35",
    "env-cmd": "10.1.0",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.7",
    "npm-check-updates": "16.14.20",
    "prettier": "3.3.2",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.13.1",
    "vue": "3.4.29",
    "vuepress": "2.0.0-rc.13"
  },
  "peerDependencies": {
    "vue": "3.4.29"
  }
}
